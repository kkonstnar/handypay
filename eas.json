{
  "cli": {
    "version": ">= 3.0.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium"
      },
      "env": {
        "EXPO_PUBLIC_API_URL": "https://handypay-backend.onrender.com",
        "STRIPE_TEST_SECRET_KEY": "$STRIPE_TEST_SECRET_KEY",
        "NEXT_PUBLIC_STRIPE_TEST_PUBLISHABLE_KEY": "$NEXT_PUBLIC_STRIPE_TEST_PUBLISHABLE_KEY",
        "EXPO_PUBLIC_APPLE_CLIENT_ID": "com.handypay.mobile",
        "APPLE_CLIENT_ID": "com.handypay.mobile",
        "EXPO_PUBLIC_GOOGLE_CLIENT_ID": "$EXPO_PUBLIC_GOOGLE_CLIENT_ID",
        "EXPO_PUBLIC_GOOGLE_CLIENT_SECRET": "$EXPO_PUBLIC_GOOGLE_CLIENT_SECRET"
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium"
      },
      "env": {
        "EXPO_PUBLIC_API_URL": "https://handypay-backend.onrender.com",
        "STRIPE_TEST_SECRET_KEY": "$STRIPE_TEST_SECRET_KEY",
        "NEXT_PUBLIC_STRIPE_TEST_PUBLISHABLE_KEY": "$NEXT_PUBLIC_STRIPE_TEST_PUBLISHABLE_KEY",
        "EXPO_PUBLIC_APPLE_CLIENT_ID": "com.handypay.mobile",
        "APPLE_CLIENT_ID": "com.handypay.mobile",
        "EXPO_PUBLIC_GOOGLE_CLIENT_ID": "$EXPO_PUBLIC_GOOGLE_CLIENT_ID",
        "EXPO_PUBLIC_GOOGLE_CLIENT_SECRET": "$EXPO_PUBLIC_GOOGLE_CLIENT_SECRET"
      }
    },
    "production": {
      "ios": {
        "resourceClass": "m-medium"
      },
      "env": {
        "EXPO_PUBLIC_API_URL": "https://handypay-backend.onrender.com",
        "STRIPE_TEST_SECRET_KEY": "$STRIPE_TEST_SECRET_KEY",
        "NEXT_PUBLIC_STRIPE_TEST_PUBLISHABLE_KEY": "$NEXT_PUBLIC_STRIPE_TEST_PUBLISHABLE_KEY",
        "EXPO_PUBLIC_APPLE_CLIENT_ID": "com.handypay.mobile",
        "APPLE_CLIENT_ID": "com.handypay.mobile",
        "EXPO_PUBLIC_GOOGLE_CLIENT_ID": "$EXPO_PUBLIC_GOOGLE_CLIENT_ID",
        "EXPO_PUBLIC_GOOGLE_CLIENT_SECRET": "$EXPO_PUBLIC_GOOGLE_CLIENT_SECRET"
      }
    }
  },
  "submit": {
    "production": {}
  }
}